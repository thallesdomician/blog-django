#: venv/lib/python3.6/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Opções avançadas"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Páginas Planas"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:9
#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:12
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr "Exemplo: '/sobre/contato/'. Lembre-se das barras no começo e no final."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor deve conter apenas letras, números, pontos, sublinhados, traços, "
"barras ou til."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:29
msgid "Example: '/about/contact'. Make sure to have a leading slash."
msgstr "Exemplo: '/sobre/contato'. Certifique-se de ter a barra no início."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:42
msgid "URL is missing a leading slash."
msgstr "Está faltando uma barra no início da URL."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a trailing slash."
msgstr "Está faltando uma barra no final da URL."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/forms.py:64
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Uma flatpage com a URL %(url)s já existe para o site %(site)s"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "título"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "conteúdo"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "habilitar comentários"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nome do template"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"Exemplo: 'flatpages/contact_page.html'. Se não for informado, será utilizado "
"'flatpages/default.html'."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "registro obrigatório"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Se estiver assinalado, apenas usuários autenticados poderão ver a página."

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:27
msgid "sites"
msgstr "sites"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:31
msgid "flat page"
msgstr "página plana"

#: venv/lib/python3.6/site-packages/django/contrib/flatpages/models.py:32
msgid "flat pages"
msgstr "páginas planas"
